import { ControllerBasicClass, HttpRenderDecorator } from '@wox/basic';
const Http = new HttpRenderDecorator();
@Http.Controller('/')
export default class <%=className%> extends ControllerBasicClass {
  constructor(ctx) {
    super(ctx);
  }

  @Http.Get('/')
  async welcome() {
    await this.ctx.render(this.ctx.Webview.Index);
  }
}
